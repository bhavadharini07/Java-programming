//Program
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
         System.out.print("Enter a number (1 to 20): ");
        int target = scanner.nextInt();
        System.out.println("Combinations that sum to " + target + ":\n");
         for (int a = 1; a <= 1; a++) {  
            for (int b = 1; b <= 1; b++) {  
                for (int i = 1; i <= 10; i++) {   
                    for (int j = 1; j <= 10; j++) {  
                        if (i + j == target) {
                            System.out.println(i + " + " + j + " = " + target);
                        }
                    }
                }
            }
        }
    }
}
//Output
Enter a number (1 to 20): 15
Combinations that sum to 15:
5 + 10 = 15
6 + 9 = 15
7 + 8 = 15
8 + 7 = 15
9 + 6 = 15
10 + 5 = 15


//Program
class Main{
     static void myMethod(int x,int y){
        System.out.println(x+y);
    }
    public static void main(String[] num){
        myMethod(1,1);
    }
}
//Output
2


