//Program
public class Main {
    public static void main(String[] args) {
        System.out.println("Squares of numbers from 1 to 100:\n");
        for (int i = 1; i <= 100; i++) {
            for (int j = 1; j <= 1; j++) {
                System.out.println(i + "^2 = " + (i * i));
            }
        }
    }
}
